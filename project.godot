[application]

config/name="Foxy Adventure"
run/main_scene="res://Scenes/intro.tscn"
boot_splash/image="res://Graphics/icon.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://Graphics/icon.png"
config/windows_native_icon="res://Graphics/icon.ico"
config/project_creator="New DEV"
config/version="1.0.0"

[autoload]

BackgroundLoad="*res://Scenes/background_load.tscn"
Console="*res://addons/godot-console-dev/src/Console.tscn"
Globals="*res://Scripts/Globals.gd"
Screenshot="*res://Scripts/screenshot.gd"
ErrorCodeServer="*res://ErrorCodeServer.gd"
ScreenDebugger="*res://addons/screen_debugger/S_ScreenDebugger.gd"
WindowTitleUpdater="*res://src/singletons/WindowTitleUpdater.gd"
SelectedObjects="*res://src/singletons/SelectedObjects.gd"
CopyPaste="*res://src/singletons/CopyPaste.gd"
EditMode="*res://src/singletons/EditMode.gd"
ExitHandler="*res://src/singletons/ExitHandler.gd"
LevelUndo="*res://src/singletons/LevelUndo.gd"
UnsaveChanges="*res://src/singletons/UnsaveChanges.gd"
